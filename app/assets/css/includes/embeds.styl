.embeded-link
  overflow hidden
  img 
    display none
    max-width 100px
    max-height 100px
    margin 0
    padding 0
    float right
    @media all and (min-width: minWidth)
      display block
  .embed-link-content
    @media all and (min-width: minWidth)
      margin-right 160px
  h2
    font-size 16px
    a
      color blackColor
  p
    font-size 12px
  
    